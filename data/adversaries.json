    "England":{
        "additional_loss_condition":{
            "title":"Proud & Mighty Capital",
            "effect":"If 7 or more TOWN/CITY are ever in a single land, the Invaders win."
        },
        "stage_2_escalation":{
            "title":"Building Boom",
            "effect":"On each board with TOWN/CITY, Build in the land with the most TOWN/CITY."
        },
        "level":{
            "0":{
                "difficulty":1,
                "fear_cards":"9 (3/3/3)",
                "game_effects":{
                    "title":"",
                    "effect":""
                }
            },
            "1":{
                "difficulty":3,
                "fear_cards":"10 (3/4/3)",
                "game_effects":{
                    "title":"Indentured Servants Earn Land",
                    "effect":"Invader Build actions affect lands without Invaders, if they are adjacent to at least 2 TOWN/CITY before the Build Action."
                }
            },
            "2":{
                "difficulty":4,
                "fear_cards":"11 (4/4/3)",
                "game_effects":{
                    "title":"Criminals and Malcontents",
                    "effect":"During Setup, on each board add 1 CITY to land #1, and 1 TOWN to land #2."
                }
            },
            "3":{
                "difficulty":6,
                "fear_cards":"13 (4/5/4)",
                "game_effects":{
                    "title":"High Immigration (I)",
                    "effect":"Put the 'High Immigration' tile on the Invader Board, to the left of 'Ravage'. The Invaders take this Build action each Invader Phase before Ravaging. Cards slide left from Ravage to it, and from it to the discard pile. Remove the tile when a Stage II card slides onto it, putting that card in the discard."
                }
            },
            "4":{
                "difficulty":7,
                "fear_cards":"14 (4/5/5)",
                "game_effects":{
                    "title":"High Immigration (full)",
                    "effect":"The extra Build tile remains out the entire game."
                }
            },
            "5":{
                "difficulty":9,
                "fear_cards":"14 (4/5/5)",
                "game_effects":{
                    "title":"Local Autonomy",
                    "effect":"TOWN/CITY have +1 Health."
                }
            },
            "6 ":{
                "difficulty":11,
                "fear_cards":"13 (4/5/4)",
                "game_effects":{
                    "title":"Independent Resolve",
                    "effect":"During Setup, add an additional FEAR to the Fear Pool per player in the game. During any Invader Phase where you resolve no Fear Cards, perform the Build from High Immigration twice. (This has no effect is no card is on the extra build slot)."
                }
            }
        }
    },
    "France (Plantation Colony)":{
        "additional_loss_condition":{
            "title":"Sprawling Plantations",
            "effect":"Before Setup, return all but 7 TOWN per player to the box. Invaders win if you ever cannot place a TOWN."
        },
        "stage_2_escalation":{
            "title":"Demand for New Cash Crops",
            "effect":"After Exploring, on each board, pick a land of the shown terrain. If it has TOWN/CITY, add 1 BLIGHT. Otherwise, add 1 TOWN."
        },
        "level":{
            "0":{
                "difficulty":2,
                "fear_cards":"9 (3/3/3)",
                "game_effects":{
                    "title":"",
                    "effect":""
                }
            },
            "1":{
                "difficulty":3,
                "fear_cards":"9 (3/3/3)",
                "game_effects":{
                    "title":"Frontier Explorers",
                    "effect":"Except during Setup: After Invaders successfully Explore into a land which had no TOWN/CITY, add 1 EXPLORER there."
                }
            },
            "2":{
                "difficulty":5,
                "fear_cards":"10 (3/4/3)",
                "game_effects":{
                    "title":"Slave Labor",
                    "effect":"During Setup, put the 'Slave Rebellion' Event under the top 3 cards of the Event Deck. After Invaders Build in a land with 2 EXPLORER or more, replace all but 1 EXPLORER there with an equal number of TOWN."
                }
            },
            "3":{
                "difficulty":7,
                "fear_cards":"11 (4/4/3)",
                "game_effects":{
                    "title":"Early Plantations",
                    "effect":"During Setup, on each board add 1 TOWN to the highest-numbered land without TOWN. Add 1 TOWN to land #1."
                }
            },
            "4":{
                "difficulty":8,
                "fear_cards":"12 (4/4/4)",
                "game_effects":{
                    "title":"Triangle Trade",
                    "effect":"Whenever Invaders Build a Coastal CITY< add 1 TOWN to the adjavent land with the fewest TOWN."
                }
            },
            "5":{
                "difficulty":9,
                "fear_cards":"13 (4/5/4)",
                "game_effects":{
                    "title":"Slow-Healing Ecosystem",
                    "effect":"When you remove BLIGHT from the board, put it here instead of onto the Blight Card. As soon as you have 3 BLIGHT per player here, move it all back to the Blight Card."
                }
            },
            "6 ":{
                "difficulty":11,
                "fear_cards":"14 (4/5/5)",
                "game_effects":{
                    "title":"Persistent Explorers",
                    "effect":"After the normal Explorel, on each board add 1 EXPLORER to a land without any. Fear Card effects never remove EXPLORER. If one would, you may instead Push that EXPLORER."
                }
            }
        }
